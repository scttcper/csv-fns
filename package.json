{
  "name": "csv-fns",
  "version": "0.0.0-placeholder",
  "description": "",
  "author": "Scott Cooper <scott.cooper@zumepizza.com>",
  "license": "MIT",
  "repository": "",
  "keywords": [
    "typescript"
  ],
  "source": "src/index.ts",
  "main": "./csv-fns.js",
  "module": "./csv-fns.m.js",
  "typings": "./src/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "lint": "tslint -t stylish -p .",
    "lint:fix": "tslint -p tsconfig.json -t stylish --fix 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "microbundle build --target node --format cjs,es -o dist",
    "postbuild": "cp package.json dist && cp LICENSE dist && cp README.md dist && rimraf dist/test",
    "build:docs": "typedoc --out docs --target es6 --theme minimal --mode file src && touch docs/.nojekyll",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:prod": "npm run test -- --coverage --no-cache",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "cd dist && semantic-release"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "23.3.9",
    "@types/node": "10.12.2",
    "jest": "23.6.0",
    "microbundle": "0.7.0",
    "prettier": "1.14.3",
    "rimraf": "2.6.2",
    "semantic-release": "15.10.7",
    "travis-deploy-once": "5.0.9",
    "ts-jest": "23.10.4",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "tslint-xo": "0.9.0",
    "typedoc": "0.13.0",
    "typescript": "3.1.6"
  },
  "release": {
    "branch": "master"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverage": true
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
